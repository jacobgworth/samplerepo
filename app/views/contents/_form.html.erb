<%= form_for(@content, :url=>'/contents/' + @content.id.to_s()) do |f| %>


  <div class="field">
    <%= f.label :url %><br />
    <%= f.text_field :url %><br />
    <%= f.label :content %><br />
    <%= f.text_field :content %><br />
    <%= f.label :headline %><br />
    <%= f.text_field :headline %><br />
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
