<!-- script to animate pullme tab -->
<script>
	window.onOff = 0;
	$(".pullme").click(function() {
		//alert("pulled");
		//$(".pullme").hide();
		if (window.onOff == 0) {
			$(".pullme").animate({
				top : "+=149px",
			}, 1000);
			$("#pullmeinfo").slideDown(1000);
			window.onOff = 1;
		} else {
			$(".pullme").animate({
				top : "-=149px",
			}, 1000);
			$("#pullmeinfo").slideUp(1000);
			window.onOff = 0;
		}
	}); 
</script>
<!-- end pullme script -->
