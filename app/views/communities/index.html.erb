<h1>Communities</h1>

<table>
  <tr>
    <th>Community</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @communities.each do |community| %>
  <tr>
    <td><%= community.communityname %> </td>
    <td><%= community.latitude %></td>
    <td><%= community.longitude %></td>
    <td><%= link_to 'Show', community %></td>
    <td><%= link_to 'Edit', edit_community_path(community) %></td>
    <td><%= link_to 'Destroy', community, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

